# The following are meant for vpn lab

getindexhtml = server:/var/log/myhttplogfile.txt : CONTAINS : GET /index.html
getlink1html = server:/var/log/myhttplogfile.txt : CONTAINS : GET /link1.html
getlink2html = server:/var/log/myhttplogfile.txt : CONTAINS : GET /link2.html

serveropenvpn = server:openvpn.stdin : PARENS : 1 : STARTSWITH : PROGRAM_ARGUMENTS is
clientopenvpn = client:openvpn.stdin : PARENS : 1 : STARTSWITH : PROGRAM_ARGUMENTS is

routertcpdump = router:tcpdump.stdin : PARENS : 1 : STARTSWITH : PROGRAM_ARGUMENTS is

wgetlink1 = client:wget.stdin : SLASH : 3 : HAVESTRING : link1.html

